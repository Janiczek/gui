module AST exposing (Expr(..), Attr)

type Expr
    = String String
    | List (Array Expr)
    | Element
        { name : String
        , id : Maybe String
        , attrs : Array Attr
        , child : Maybe Expr
        }
    | Stdin
    | Stdout
    | File { path : String }
    | Id String
    | Function
        { src : Expr
        , dst : Expr
        }


type alias Attr = { name : String
    , value : Expr }